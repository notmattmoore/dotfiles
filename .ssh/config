Host *
  ControlPersist 30m
  ControlPath ~/.ssh/master-%r@%n:%p

Host alpha.alias
  HostName alpha.lexicon
  ProxyJump alephnull.ddns.net
Host beta.alias
  HostName beta.lexicon
  ProxyJump alephnull.ddns.net
Host omega.alias
  HostName alephnull.ddns.net
