Host phi
  Port 8022
Host kappa
  Port 2222
  User root
  IdentityFile ~/.ssh/rsync
Host rho
  User root

Host github
  Hostname github.com
  User git
  IdentityFile /home/mm/.ssh/id_ed25519
  IdentitiesOnly yes

# rsync {{{
Host *.rsync
  IdentityFile ~/.ssh/rsync
Host alephnull.rsync
  HostName alephnull.ddns.net
Host alpha.rsync
  HostName alpha
Host beta.rsync
  HostName omega
Host omega.rsync
  HostName omega
